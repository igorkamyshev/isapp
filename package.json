{
  "name": "isapp",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start:back:dev": "NODE_ENV=development nodemon --config ./back/nodemon.json",
    "start:back:debug": "NODE_ENV=development nodemon --config nodemon-debug.json",
    "prestart:back:prod": "rimraf dist/back && tsc -p ./back",
    "start:back:prod": "pm2 start ./back/pm2.config.js",
    "start:front:dev": "NODE_ENV=development node front/server.js",
    "prestart:front:prod": "next build ./front",
    "start:front:prod": "pm2 start ./front/pm2.json"
  },
  "dependencies": {
    "@nestjs/common": "^5.5.0",
    "@nestjs/core": "^5.5.0",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-typescript": "^1.1.1",
    "args-parser": "^1.1.0",
    "express": "^4.16.4",
    "next": "^7.0.2",
    "next-compose-plugins": "^2.1.1",
    "next-routes": "^1.4.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.3.3",
    "tsconfig-paths": "^3.7.0"
  },
  "devDependencies": {
    "@types/react": "^16.7.17",
    "babel-plugin-module-resolver": "^3.1.1",
    "nodemon": "^1.18.9",
    "pm2": "^3.2.4",
    "ts-node": "^7.0.1",
    "typescript": "^3.2.2"
  }
}
